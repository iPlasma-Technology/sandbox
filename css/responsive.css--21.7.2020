     .android_sec {
        width: 50%;
        position: relative;
        left: 0;
        top: 0;
        text-align: left;
         padding-right: 20px;
}
     .ios_sec {
        width: 50%;
        position: relative;
        left: 0;
        top: 0;
        text-align: left;
         padding-right: 20px;
}
    .android_ios_section > img{
        display: none;
    }
    .android_ios_section{
        width: 100%;
        display: flex;
    }
    
    .android_sec h3{
        font-size: 25px;
    }
    .android_sec span {
        margin-bottom: 15px;
        font-size: 15px;
    }
    .android_sec a img{
        width: 130px;
    }
    .android_sec:before{
        display: none;
    }
    
    .ios_sec:before{
        display: none;
    }
    .ios_sec h3{
        font-size: 25px;
    }
    .ios_sec span {
        margin-bottom: 15px;
        font-size: 15px;
    }
    .ios_sec a img{
        width: 130px;
    }
    
    .productivity_section h2{
        font-size: 35px;
        line-height: 45px;
    }
    .logo img{
        width: 150px;
    }
    .navigation .navbar-nav>li{
        margin: 0 20px;
    }
    .banner h2{
        font-size: 40px;
    }
    .banner{
        margin-top: 20px;
    }
    .top_blog_left{
        padding: 20px;
    }
    .top_blog_right{
        padding: 20px;
    }
    .top_blog{
        margin-top: 25px;
    }
    .testimonials_section{
        margin-top: 30px;
    }
    .testimonials_left{
        padding-right: 0;
    }
    .video_text{
        padding-right: 0;
    }
    .latest_news_text h4{
        font-size: 18px;
    }
    .latest_news_text .description_news{
        font-size: 14px;
    }
    .latest_news_text{
        padding-left: 0;
        min-height: 275px;
    }
    .footer_logo p{
        font-size: 16px;
    }
    .footer_download_app_btn a img{
        width: 92px;
    }
/*    news*/
    .news_latter_field_box{
        padding: 0;
    }
    .input_fields{
        width: 250px;
    }
    .news_banner_inner{
        width: 100%;
    }
/*    news*/
    

}
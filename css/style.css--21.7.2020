    margin-left: 0;
}
.video_page_banner .carousel-control.right{
    background: none;
    margin-right: 0;
}
.video_page_banner .carousel-control{
    width: auto;
    display: flex;
    align-items: center;
}
.video_page_banner .carousel-indicators {

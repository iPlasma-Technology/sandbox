}

.text_slide{
    text-align: center;
}
.text_slide h2{
    color: #515151;
    font-size: 60px;
    margin: 0;
    padding: 0;
    font-weight: 500;
}

.text_slide h3{
    color: #000000;
    font-size: 60px;
    margin: 0;
    padding: 0;
    font-weight: bold;
}
.text_slide ul{
   margin: 20px 0 0 0;
    padding: 0;
}
.text_slide ul li{
    list-style-type: none;
    font-size: 16px;
    color: #a2a2a2;
    margin: 0;
    padding: 0;

.nav_download{
    display: flex;
    align-items: center;
}
.navigation .navbar-default{
    background: none;
    border: none;
}
.navigation .navbar-nav>li{
    margin: 0 32px;
}
.navigation .navbar-nav>li>a{
    font-size: 16px;
    color: #000000;
    padding: 0;
    margin: 0;
    position: relative;
    
}
.navigation .navbar-default .navbar-nav>.active>a,
.navigation .navbar-default .navbar-nav>.active>a:hover,
.navigation .navbar-default .navbar-nav>.active>a:focus{
       color: #000;
       background: none;
}

.navigation .navbar-nav>li>a:before{
    width: 0;
    height: 2px;
    background: #ef5e3c;
    content: "";
    position: absolute;
    left: -14px;
    bottom: -8px;
    transition: .5s;
}


.navigation .navbar-nav>li.active>a:before{
    width: 50px;
    height: 2px;
    background: #ef5e3c;
    content: "";
    position: absolute;
    left: -14px;
    bottom: -8px;
}
.navbar-default .navbar-nav>li>a:hover:before, .navbar-default .navbar-nav>li>a:focus:before{
    width: 50px;
}

/*modal*/
.app_modal{
    text-align: center;
    padding: 20px 15px 20px;
}


.app_modal .form-control::-webkit-input-placeholder { /* Chrome/Opera/Safari */
  color: #555;
  opacity: 1;
}
.app_modal .form-control::-moz-placeholder { /* Firefox 19+ */
  color: #555;
  opacity: 1;
}
.app_modal .form-control:-ms-input-placeholder { /* IE 10+ */
  color: #555;
  opacity: 1;
}
.app_modal .form-control:-moz-placeholder { /* Firefox 18- */
  color: #555;
  opacity: 1;
}
.modal-content button.close{
    position: absolute;
    right: -7px;
    top: -6px;
    width: 25px;
    height: 25px;
    border-radius: 50%;
    background: #eb370a;
    color: #fff;
    z-index: 999;
    opacity: 1;
    outline: 0;
    font-size: 17px;

}
.app_modal h1{
    font-size: 25px;
    font-weight: bold;
    margin: 0 0 5px 0;
    padding: 0;
}
.app_modal p{
    color: #858585;
    font-family: 'Muli', sans-serif;
    margin: 0 0 20px 0;
    padding: 0;
    font-size: 16px;
}
.modal_logo{
